@model Oef2.Models.Registration
<h1>New Registration</h1>
<h3>Registration</h3>
<div class="col-lg-12">
    <form asp-action="Add" asp-controller="rapport" method="post">
        <div class="form-group">
            <label for="name">Name</label>
            <input asp-for="@Model.Naam" class="form-control" />
        </div>
        <div class="form-group">
            <label for="firstname">Firstname</label>
            <input asp-for="@Model.Voornaam" class="form-control" />
        </div>
        <div class="form-group">
            <label>Age</label>
            <input asp-for="@Model.Leeftijd" class="form-control" />
        </div>
        <div class="form-group">
            <label>Email</label>
            <input asp-for="@Model.Email" class="form-control" />
        </div>

        <input type="submit" name="Save" value="Add" class="btn" />
    </form>
</div>
